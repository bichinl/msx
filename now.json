{
  "version": 2,
  "name": "msx",
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server"
    }
  ],
  "files": ["./msx/**/*"],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/msx/(.*)",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}
